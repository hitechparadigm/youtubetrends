{
  "name": "youtube-automation-platform",
  "version": "1.3.0",
  "description": "AI-Powered YouTube Video Generation & Publishing System with Audio Integration",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev:generate": "node tools/development/test-video-generation-only.js",
    "dev:test-complete": "node tools/development/test-complete-workflow.js",
    "dev:test-upload": "node tools/development/test-youtube-upload.js",
    "dev:test-audio": "node tools/development/test-audio-timing.js",
    "deploy": "node tools/deploy/deploy-production-ready.js",
    "deploy:scheduler": "node tools/deploy/deploy-scheduler.js",
    "deploy:infrastructure": "node tools/deploy/deploy-complete-solution.js",
    "setup:youtube": "node tools/deploy/setup-youtube-api.js",
    "manage:scheduler": "node tools/management/manage-scheduler.js",
    "manage:verify": "node tools/management/verify-scheduler.js",
    "manage:analyze": "node tools/management/analyze-project-metrics.js",
    "clean": "rm -rf src/lambda/*/dist src/lambda/*/node_modules node_modules coverage",
    "lint": "eslint src tools tests --ext .js,.ts",
    "lint:fix": "eslint src tools tests --ext .js,.ts --fix",
    "format": "prettier --write \"src/**/*.{js,ts}\" \"tools/**/*.{js,ts}\" \"tests/**/*.{js,ts}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts}\" \"tools/**/*.{js,ts}\" \"tests/**/*.{js,ts}\"",
    "prepare": "husky install || true"
  },
  "keywords": [
    "youtube",
    "automation",
    "ai",
    "video-generation",
    "aws",
    "bedrock",
    "polly",
    "serverless",
    "content-creation",
    "seo"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/youtube-automation-platform.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/youtube-automation-platform/issues"
  },
  "homepage": "https://github.com/yourusername/youtube-automation-platform#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-cloudformation": "^3.902.0",
    "@aws-sdk/client-cloudwatch": "^3.0.0",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-lambda": "^3.901.0",
    "@aws-sdk/client-mediaconvert": "^3.901.0",
    "@aws-sdk/client-polly": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-scheduler": "^3.901.0",
    "@aws-sdk/client-secrets-manager": "^3.0.0",
    "@aws-sdk/client-ssm": "^3.901.0",
    "@aws-sdk/lib-dynamodb": "^3.902.0",
    "googleapis": "^128.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/yourusername"
  }
}
