# YouTube Automation Platform - Environment Variables Template
# Copy this file to .env and fill in your values

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-account-id

# YouTube API
YOUTUBE_API_KEY=your-youtube-api-key
YOUTUBE_CLIENT_ID=your-client-id
YOUTUBE_CLIENT_SECRET=your-client-secret

# S3 Buckets
VIDEO_BUCKET=youtube-automation-videos-your-account-us-east-1
LUMA_BUCKET=youtube-automation-luma-your-account

# Lambda Configuration
LAMBDA_TIMEOUT=900
LAMBDA_MEMORY=2048

# Development
NODE_ENV=development
LOG_LEVEL=info

# Testing
MOCK_VIDEO_GENERATION=false
TEST_MODE=false