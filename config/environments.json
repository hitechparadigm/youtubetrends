{
  "development": {
    "region": "us-east-1",
    "stackName": "YoutubeAutomationPlatformStack-Dev",
    "environment": "development",
    "costOptimization": {
      "enableS3LifecyclePolicies": false,
      "dynamoDbBillingMode": "ON_DEMAND",
      "lambdaReservedConcurrency": 5
    },
    "monitoring": {
      "enableDetailedMonitoring": false,
      "logRetentionDays": 7
    }
  },
  "staging": {
    "region": "us-east-1",
    "stackName": "YoutubeAutomationPlatformStack-Staging",
    "environment": "staging",
    "costOptimization": {
      "enableS3LifecyclePolicies": true,
      "dynamoDbBillingMode": "ON_DEMAND",
      "lambdaReservedConcurrency": 10
    },
    "monitoring": {
      "enableDetailedMonitoring": true,
      "logRetentionDays": 14
    }
  },
  "production": {
    "region": "us-east-1",
    "stackName": "YoutubeAutomationPlatformStack",
    "environment": "production",
    "costOptimization": {
      "enableS3LifecyclePolicies": true,
      "dynamoDbBillingMode": "ON_DEMAND",
      "lambdaReservedConcurrency": 50
    },
    "monitoring": {
      "enableDetailedMonitoring": true,
      "logRetentionDays": 30,
      "enableXRayTracing": true
    },
    "security": {
      "enableVpcFlowLogs": true,
      "enableCloudTrail": true
    }
  }
}